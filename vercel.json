{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "redirects": [
      {
        "source": "/:match*",
        "destination": "/index.html",
        "statusCode": 200,
        "permanent": false
      }
    ]
  }
  